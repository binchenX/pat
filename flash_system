# install system, part 0: system.img.raw_aa
fatload usb 0:1 0x8000000 system.img.raw_aa
mmc write 0x8000000 0x16000 0x96000
# install system, part 1: system.img.raw_ab
fatload usb 0:1 0x8000000 system.img.raw_ab
mmc write 0x8000000 0xac000 0x96000
# install system, part 2: system.img.raw_ac
fatload usb 0:1 0x8000000 system.img.raw_ac
mmc write 0x8000000 0x142000 0x96000
# install system, part 3: system.img.raw_ad
fatload usb 0:1 0x8000000 system.img.raw_ad
mmc write 0x8000000 0x1d8000 0x3e000